[GENERAL]
polynomialOrder = 3
stopAt = endTime
endTime = 10.0
dt = 1.0e-2
timeStepper = tombo2
subCyclingSteps = 0

regularization = hpfrt + nModes=1 + scalingCoeff=10
constFlowRate = meanVelocity=1.0 + direction=Z

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = wall
viscosity = 1.0e-4
density = 1.0
residualTol = 1e-06

[CASEDATA]
Re_tau = 550
zLength = 6